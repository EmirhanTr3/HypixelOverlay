{
  "schemaVersion": 1,
  "id": "hypixeloverlay",
  "version": "${version}",
  "name": "HypixelOverlay",
  "description": "Shows information about players in your hypixel game.",
  "authors": ["EmirhanTr3"],
  "contact": {
    "sources": "https://github.com/EmirhanTr3/HypixelOverlay",
    "homepage": "https://modrinth.com/mod/hypixeloverlay",
    "issues": "https://github.com/EmirhanTr3/HypixelOverlay/issues"
  },
  "license": "GPL-3.0-only",
  "icon": "assets/hypixeloverlay/icon.png",
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.modrinth": "https://modrinth.com/mod/hypixeloverlay",
        "modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/hypixeloverlay"
      }
    }
  },
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "xyz.emirdev.hypixeloverlay.HypixelOverlayDataGenerator"
    ],
    "client": [
      "xyz.emirdev.hypixeloverlay.HypixelOverlayClient"
    ],
    "modmenu": [
      "xyz.emirdev.hypixeloverlay.ModMenuIntegration"
    ]
  },
  "mixins": [
    "hypixeloverlay.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
